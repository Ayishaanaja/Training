package pack;

import java.util.Scanner;

public class HexOct {
public static void main(String[] args) {
	String hex="1B";
	int A=10;
	int B=11;
	int C=12;
	int D=13;
	int E=14;
	int F=15;
	for(int i=0;i<hex.length();i++)
	{
		char s=hex.charAt(i);
		if(s==A||s==B||s==C||s==D||s==E||s==F)
		{
			decimal=s;
		}
		else
		{
			decimal=s*16;
		}
	}
}
}
