package pack;

import java.util.Scanner;

public class pattern {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter length : ");
	int num=sc.nextInt();
	for(int i=0;i<num;i++)
	{
		int a=1;
		for(int j=0;j<i+1;j++)
		{
			System.out.println(a);
			a++;
		}
	}
}
}
