package inheritenc;

import outer.Main;

class A{
	A(){
		System.out.println("Class A constructor");
	}
	A(int x)
	{
		System.out.println("The value of x is "+x);
	}
	
}
public class B extends A{
B(){
	System.out.println("Class B constructor");
}
	public static void main(String[] args)
	{
	new B();	
	}
	}

