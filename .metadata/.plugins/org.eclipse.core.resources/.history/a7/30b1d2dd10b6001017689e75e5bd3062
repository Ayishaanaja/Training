package pack;

public class avgarray {
public static void main(String[] args) {
	int mark[]= {22,33,44,55,66};
	int sum=0,count=0;
	for(int i=0;i<mark.length;i++)
	{
		sum+=mark[i];
	}
	System.out.println(sum/mark.length);
	System.out.println("--------------------------------------------------------");
	for(int i=0;i<mark.length;i++)
	{
		if(mark[i]<40)
		{
			count++;
		}
	}
	System.out.println("subjects failed : "+count);
}
}
