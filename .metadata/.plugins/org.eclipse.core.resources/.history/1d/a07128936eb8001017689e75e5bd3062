package inheritenc;

public class Tiger extends Animal 
{
void run()
{
	System.out.println("runs fast");
}
void eat()
{
	System.out.println("eats only non veg");
}
public static void main(String[] args) {
	Tiger t=new Tiger();
	t.run();
	t.eat();
	System.out.println("Tiger has "+t.legs+" legs");
}
}
