package pack;

public class stat {
	static
	{
		System.out.println("welcome to");
	}
public static void main(String[] args) {
	System.out.println("java");
	System.out.println("---------------------------------------------------------------");
	First:
		for(int i=0;i<5;i++)
		{
			Second:
				for(int j=0;j<5;j++)
				{
					if (j==2)
					{
						break First;
					}
				System.out.println("hi"+j);
				}	
		}
			
}
}
